<button id="testinsert">Click Here To Test Insert</button>
<br>
<input id="scrapetest-input">
<button id="testscrape">Click to Test Onion Scraper</button>

<script type="text/javascript">
    $("#testinsert").on("click", function(event) {
        event.preventDefault();
        $.ajax({
            method: "POST",
            url: "/developer/test/insert",
            data: {
                title: "ACK",
                date: 69,
                url: "http:whatever",
                subtitle: "This is a test article",
                articleBody: "Lorem ipsum yadda yadda yadda",
                source: "My Mom"
            }
        }).then(function(err, res) {
            if (err) console.log(err);
            console.log(res);
        })
    })

    $("#testscrape").on("click", function(event) {
        var testValue = $("#scrapetest-input").val();
        event.preventDefault();
        $.ajax({
            method: "GET",
            url: "/developer/test/scrape/" + testValue
        })
    })
</script>